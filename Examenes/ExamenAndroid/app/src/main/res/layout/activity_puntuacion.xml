<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityPuntuacion">

    <TextView
        android:id="@+id/puntuacionMasAlta"
        android:layout_width="262dp"
        android:layout_height="132dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="123dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="62dp"
        android:fontFamily="monospace"
        android:text="PUNTUACION MAS ALTA OBTENIDA"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="35dp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="76dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="259dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/btnVolverPuntuacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/puntuacionMasAlta" />

    <Button
        android:id="@+id/btnVolverPuntuacion"
        android:layout_width="105dp"
        android:layout_height="56dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="17dp"
        android:fontFamily="monospace"
        android:text="Volver"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />
</androidx.constraintlayout.widget.ConstraintLayout>