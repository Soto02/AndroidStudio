<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityInicioSesion">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="166dp"
        android:layout_height="123dp"
        android:layout_marginStart="122dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="123dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/descarga_removebg_preview" />

    <TextView
        android:id="@+id/textViewInicioContraseña"
        android:layout_width="140dp"
        android:layout_height="42dp"
        android:layout_marginStart="79dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="192dp"
        android:fontFamily="@font/audiowide"
        android:text="Contraseña"
        android:textAlignment="viewStart"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textBoxInicioUsuario" />

    <Button
        android:id="@+id/btnConfirmarCuenta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="131dp"
        android:fontFamily="@font/audiowide"
        android:onClick="onClick"
        android:text="Iniciar sesion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvIRregistrarse" />

    <EditText
        android:id="@+id/textBoxInicioContraseña"
        android:layout_width="252dp"
        android:layout_height="44dp"
        android:layout_marginStart="79dp"
        android:layout_marginEnd="80dp"
        android:backgroundTint="#8C8C8C"
        android:ems="10"
        android:fontFamily="@font/audiowide"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewInicioContraseña" />

    <EditText
        android:id="@+id/textBoxInicioUsuario"
        android:layout_width="252dp"
        android:layout_height="43dp"
        android:layout_marginStart="79dp"
        android:layout_marginEnd="80dp"
        android:backgroundTint="#8C8C8C"
        android:ems="10"
        android:fontFamily="@font/audiowide"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewInicioUsuario" />

    <TextView
        android:id="@+id/textViewInicioUsuario"
        android:layout_width="113dp"
        android:layout_height="43dp"
        android:layout_marginStart="79dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="219dp"
        android:fontFamily="@font/audiowide"
        android:text="Usuario"
        android:textAlignment="viewStart"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <TextView
        android:id="@+id/tvIRregistrarse"
        android:layout_width="151dp"
        android:layout_height="24dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="130dp"
        android:fontFamily="@font/audiowide"
        android:onClick="onClick"
        android:text="Registrate aqui"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textBoxInicioContraseña" />

</androidx.constraintlayout.widget.ConstraintLayout>